<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TableColumn?>
<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">

    <HBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
        <VBox spacing="10">

            <HBox spacing="10">
                <Button fx:id="btnAnalysis" prefHeight="30.0" prefWidth="75.0" text="Run analysis" onAction="#startAnalysis" />
            </HBox>

            <TextArea prefHeight="300.0" prefWidth="600.0"  fx:id="txtInput" />


         <TableView prefHeight="250.0" prefWidth="200.0" fx:id="tableSymbols">
             <TableColumn text="col" >

             </TableColumn>
         </TableView>
            <HBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </HBox.margin>
        </VBox>

        <VBox spacing="10.0">
            <TableView prefHeight="300.0" prefWidth="650.0" >
                <VBox.margin>
                    <Insets right="20.0" top="70.0" />
                </VBox.margin></TableView>

            <ListView prefHeight="250.0" prefWidth="650.0">
                <VBox.margin>
                   <Insets right="20.0" />
                </VBox.margin>
            </ListView>

        </VBox>

    </HBox>

   <TextArea prefHeight="100.0" prefWidth="200.0">
      <VBox.margin>
         <Insets bottom="5" left="20.0" right="20.0" top="5" />
      </VBox.margin>
   </TextArea>

</VBox>
